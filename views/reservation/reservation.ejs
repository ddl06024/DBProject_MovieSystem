<body>
    <div id="app">
        <header class ="TOP">
            <div class="m-4">
                <a class="head_icon" href="/" style="text-decoration: none;">
                    <span>G</span>
                    <span>I</span>
                    <span>G</span>
                    <span>A</span>
                    <span>B</span>
                    <span>O</span>
                    <span>X</span>
                </a>
            </div>
            <div id="TITLE">
                <div id ="FIRST" type ="button" onclick="Toregister()">예약하기</div>
                <script type="text/javascript">
                function Toregister(){
                    location.href = "/reservation2";
                }
                </script>
                <!-- <a id ="FIRST2" href = "/mypage/<%= currentUser.USER_ID %>">마이페이지</a> -->
                <div id="FIRST2" type ="button" onclick="Tomypage()">마이페이지</div>
                <script type="text/javascript">
                 function Tomypage(){
                     location.href = "/mypage/<%= currentUser.USER_ID %>";
                 }
                 </script>
                  <div id ="FIRST1" type ="button" onclick="Tosnacklist()">스낵구매</div>
                  <script type="text/javascript">
                  function Tosnacklist(){
                      location.href = "/snack/<%= currentUser.USER_ID %>";
                  }
                  </script>
                <div id="SECOND" type ="button" onclick="Tologin()">로그아웃</div>
                <script type="text/javascript">
                    function Tologin(){
                        location.href = "/logout";
                      }
                </script> 
            </div>
         </header>
        <div id="title">예매하기
        </div>
        <div id="CENTER">
        </div>
        <form action="/reservation2/reservate" method="post" @submit="submitWithValidation(event)">
            <div id="bx1">
                <div id="bx1_bx1">
                    <div id="bx1_bx1_tx1" style="font: normal normal bold 30px/35px NanumSquareRound; color: #FFFFFF;">영화선택</div>
                </div>
                <div id="bx1_bx2" style="font-family: NanumSquareRound; font-weight: bold; font-size: 30px; color: #000000;">
                    <div id="bx1_bx2_tx1">예매순</div>
                    <div class="movie_title" style="overflow: scroll; width: 380; height: 330px;">
                        <% if(!movie_info){ %>
                            <% movies.forEach(function(movie){ %>
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="movie_num" v-model="movie_num" value="<%= movie.MOVIE_NUM %>" id="flexRadioDefault">
                                    <label class="form-check-label" for="flexRadioDefault">
                                    <%= movie.MOVIE_TITLE %>
                                    </label>
                                </div>
                            <% }) %>
                        <% } else { %>
                            <% movies.forEach(function(movie){ %>
                                <div class="form-check">
                                    <input class="form-check-input" type="radio" name="movie_num" v-model="movie_num" value="<%= movie.MOVIE_NUM %>" id="flexRadioDefault">
                                    <label class="form-check-label" for="flexRadioDefault<%= movie.MOVIE_NUM %>">
                                    <%= movie.MOVIE_TITLE %>
                                    </label>
                                </div>
                            <% }) %>
                        <% } %>
                    </div>
                </div>
            </div>
            <div id="bx2">
                <div id="bx2_bx1">
                    <div id="bx2_bx1_tx1" style="font: normal normal bold 30px/35px NanumSquareRound; color: #FFFFFF;">극장선택</div>
                </div>
                <div id="bx2_bx2" style="font-family: NanumSquareRound; font-weight: bold; font-size: 30px; color: #000000;">
                    <div id="bx2_bx2_tx1">전체극장</div>
                </div>
                <div id="bx2_tx1" style="font-family: NanumSquareRound; font-size: 25px; color: #0909FF;">동대문구</div>
                <div id="bx2_tx2" style="font-family: NanumSquareRound; font-size: 25px; color: #000000;">GIGABOX-청량리</div>
            </div>
            <div id="bx3">
                <div id="bx3_bx1">
                    <div id="bx3_bx1_tx1" style="font: normal normal bold 30px/35px NanumSquareRound; color: #FFFFFF;">날짜선택</div>
                </div>
                <div id="bx3_bx2" style="font-family: NanumSquareRound; font-weight: bold; font-size: 30px; color: #000000;">
                    <div id="bx3_bx2_tx1">2021년</div>
                </div>
                <p id="date">
                    <% if(!date){ %>
                        <input type="date" name="date" min='currentDate' max="2021-12-31" v-model="date">
                    <% } else { %>
                        <input type="date" name="date" min='currentDate' max="2021-12-31" v-model="date" value="<%= date %>">
                    <% } %>
                    <!--
                    <span class="invalid-feedback d-block" v-if="errors.movie_num" style="font-size: 15px;">{{ errors.movie_num }}</span>
                    <span class="invalid-feedback d-block" v-if="errors.date" style="font-size: 15px;">{{ errors.date }}</span>
                    -->
                </p>
                <button class="btn btn-outline-secondary" type="submit" onclick="confirm('상영 일정을 조회 하시겠습니까?')"style="position: absolute; top:350px;">상영일정 조회</button>
                <!--
                <form id="date">
                    <p><input type="date" min='currentDate' max="2021-05-31"></p>
                </form>
                -->
            </div>
        </form>
        <form action="/reservation2/reservate2" method="post" @submit="submitWithValidation2(event)">
            <% if(movie_info && date){ %>
                <input type="hidden" name="movie_num" value="<%= movie_info.MOVIE_NUM %>">
                <input type="hidden" name="date" value="<%= date %>">
            <% } %>
            <div id="bx4">
                <% if(movie_info != null ){ %>
                    <div id="bx4_info1">영화 <%= movie_info.MOVIE_TITLE %></div>
                <% } else { %>
                    <div id="bx4_info1">영화</div>
                <% }%>
                <div id="bx4_info2">날짜 <%= date %></div>
                <div id="bx4_info3">인원 성인 {{adults}} 청소년 {{kids}}</div>
                <div id="bx4_info4">좌석</div>
                <button id="bt_reserve" onclick="confirm('예약하시겠습니까?')">예약하기</button>
            </div>
            <div id="bx5" style="overflow: scroll;">
                <div id="bx5_bx1">
                    <div id="bx5_bx1_tx1" style="font: normal normal bold 30px/35px NanumSquareRound; color: #000000;">상영시간표</div>
                </div>
                <div class="mt-2" id="bx5_tx1_2" style="font: normal normal bold 30px/35px NanumSquareRound; color: #000000;">극장명</div>
                <div style="margin-top: 30px;">   
                    <% boxs.forEach(function(box){ %>
                        <% var check = false; %>
                        <% schedules.forEach(function(schedule){ %>
                            <% if(box.SCREEN_BOX_NUM == schedule.SCREEN_BOX_NUM){%>
                                <% check = true; %>
                            <% } %>
                        <% }) %>
                        <% if(check == true){ %>
                            <div class="ms-3 mt-2" id="bx5_tx2" style="font: normal normal bold 30px/35px NanumSquareRound; color: #000000;"><%=box.SCREEN_BOX_NAME %></div>
                            <% schedules.forEach(function(schedule){ %>
                                <% if(box.SCREEN_BOX_NUM == schedule.SCREEN_BOX_NUM){%>
                                    <div id="bx5_bt" class="btn-group ms-3" role="group" aria-label="Basic radio toggle button group" >
                                        <input type="radio" class="btn-check" name="schedule" v-model="movie_box" id="<%= schedule.SCREEN_SCHEDULE_NUM %>" value="<%= schedule.SCREEN_SCHEDULE_NUM %>" autocomplete="off">
                                        <label class="btn btn-outline-secondary" for="<%= schedule.SCREEN_SCHEDULE_NUM %>" style="font-size:20px; font-family: NanumGothicCoding; font-weight: 800; "><%= schedule.SCREEN_START_TIME %></label>
                                    </div>
                                <% } %>
                            <% }) %>        
                        <% } %>
                    <% }) %>
                </div> 
            </div>
            <div id="bx6">
                <div id="bx6_bx1">
                    <div id="bx6_bx1_tx1" style="font: normal normal bold 30px/35px NanumSquareRound; color: #000000;">인원선택</div> 
                </div>
                <% if(schedules.length != 0) { %>
                    <div id="bx6_tx1" style="font: normal normal bold 30px/35px NanumSquareRound; color: #000000;">성인</div>
                    <div id="bx6_bt1" class="btn-group" role="group" aria-label="Basic radio toggle button group">

                        <input type="radio" class="btn-check" name="adults" id="btnradio0" v-model="adults" value="0" autocomplete="off">
                        <label class="btn btn-outline-secondary" for="btnradio0" style="font-family: NanumGothicCoding; font-weight: 800; ">0</label>
            
                        <input type="radio" class="btn-check" name="adults" id="btnradio1" v-model="adults" value="1" autocomplete="off">
                        <label class="btn btn-outline-secondary" for="btnradio1" style="font-family: NanumGothicCoding; font-weight: 800; ">1</label>
                        
                        <input type="radio" class="btn-check" name="adults" id="btnradio2" v-model="adults" value="2"  autocomplete="off">
                        <label class="btn btn-outline-secondary" for="btnradio2" style="font-family: NanumGothicCoding; font-weight: 800; ">2</label>
                        
                        <input type="radio" class="btn-check" name="adults" id="btnradio3" v-model="adults" value="3" autocomplete="off">
                        <label class="btn btn-outline-secondary" for="btnradio3" style="font-family: NanumGothicCoding; font-weight: 800; ">3</label>

                        <input type="radio" class="btn-check" name="adults" id="btnradio4" v-model="adults" value="4" autocomplete="off">
                        <label class="btn btn-outline-secondary" for="btnradio4" style="font-family: NanumGothicCoding; font-weight: 800; ">4</label>

